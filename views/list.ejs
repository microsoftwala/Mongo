<%- include("header") -%>
           
    <div class="box">
                <% for (var i =0; i < New.length;  i++ ) { %>
                      <div class="item">
                        <input type="checkbox">
                        <p><%= New[i] %></p>
                      </div>
                <% } %>


            <form class="item" action="/" method="post">
                <button type="submit" name="listrem" value=<%= listTitle%>>-</button>
                <input type="text" name="newItem" placeholder="newItem" autocomplete="off">
                <button type="submit" name="listadd" value=<%= listTitle%>>+</button>
            </form>
        </div>
    <div class="a">
        <a href="/work" style="text-decoration:none; color:rgb(0, 0, 0); font-weight:bold;width:fit-content; background-color:rgb(204, 204, 204); border-radius:5px">Work list</a>
        <a href="/" style="text-decoration:none;color:rgb(0, 0, 0);font-weight:bold; margin-left:50px; width:fit-content; background-color:rgb(204, 204, 204); border-radius:5px">Normal-list</a>
    </div>
<%- include("footer") -%>